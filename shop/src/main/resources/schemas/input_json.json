 {
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "object",
   "title": "The Root Schema",
   "required": [
     "id",
     "id_user",
     "date",
     "id_good"
   ],
   "properties": {
     "id": {
       "$id": "#/properties/id",
       "type": "integer",
       "title": "The Id Schema",
       "default": 0,
       "examples": [
         2
       ]
     },
     "id_user": {
       "$id": "#/properties/id_user",
       "type": "integer",
       "title": "The Id_user Schema",
       "default": 0,
       "examples": [
         2
       ],
       "pattern": "^(.*)$"
     },
     "date": {
       "$id": "#/properties/date",
       "type": "string",
       "title": "The Date Schema",
       "default": "",
       "examples": [
         "2019/11/30"
       ],
       "pattern": "^(.*)$"
     },
     "id_good": {
       "$id": "#/properties/id_good",
       "type": "integer",
       "title": "The Id_good Schema",
       "default": 0,
       "examples": [
         2
       ]
     }
   }
 }